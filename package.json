{
  "name": "inter-letter-spacing",
  "description": "Sketch plugin that dynamically sets letter spacing of layers using the Inter font.",
  "version": "1.0.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "inter-letter-spacing",
    "manifest": "src/manifest.json",
    "main": "inter-letter-spacing.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "Giliam Verheide"
}
